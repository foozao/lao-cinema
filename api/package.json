{
  "name": "lao-cinema-api",
  "version": "1.0.0",
  "description": "Backend API for Lao Cinema streaming platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "link-video": "tsx src/scripts/link-video.ts"
  },
  "keywords": [
    "fastify",
    "api",
    "streaming",
    "movies"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.1",
    "@google-cloud/storage": "^7.18.0",
    "@sentry/node": "^10.32.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.38.3",
    "fastify": "^4.28.1",
    "postgres": "^3.4.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^4.0.9",
    "drizzle-kit": "^0.24.0",
    "supertest": "^7.1.4",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^4.0.9"
  }
}
